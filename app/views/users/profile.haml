.container
	.content-container
		#user_info
			%h1
				= current_user.username

			%h3
				= current_user.email

		#links-container
			%ul
				- @links.each do |link|
					= partial :link, :locals => {:link => link}

		%ul
			- @favourites.each do |link|
				= partial :link, :locals => {:link => link}